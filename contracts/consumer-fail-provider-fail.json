{
  "consumer": {
    "name": "consumer-fail"
  },
  "interactions": [
    {
      "description": "Should fail without query string",
      "providerStates": [
        {
          "name": "Response with 400"
        }
      ],
      "request": {
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "GET",
        "path": "/"
      },
      "response": {
        "body": {
          "errors": [
            "some string"
          ]
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.errors": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            },
            "$.errors[*]": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {}
        },
        "status": 400
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "12.2.0"
    },
    "pactRust": {
      "ffi": "0.4.15",
      "models": "1.1.19"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "provider-fail"
  }
}